day_work_entries.workEntryDate as Date,

day_work_entries.workHour as WorkHours,

users.name as Name,

projects.name as Project,

sub_categories.name as SubCategory,

work_details.name as WorkDetail,

day_work_entries.workComment as Comments


FROM  

day_work_entries

LEFT JOIN users ON 
day_work_entries.user_id = users.id

LEFT JOIN projects ON 
day_work_entries.project_id = projects.id

LEFT JOIN sub_categories ON 
day_work_entries.subcat_id = sub_categories.id

LEFT JOIN work_details ON 
day_work_entries.workDetail_id = work_details.id

WHERE 1 = 1 

[ AND day_work_entries.user_id in ( {name} ) ]

[ AND  day_work_entries.workEntryDate &gt;=  {workEntryDate,RANGE1} AND  day_work_entries.workEntryDate &lt;=  {workEntryDate,RANGE2} ]

[ AND day_work_entries.project_id  in ( {Project} ) ]

[ AND day_work_entries.subcat_id  in ( {SubCategory} ) ]

[ AND day_work_entries.workDetail_id  in ( {WorkDetail} ) ]

</SQLRaw></SQL><QueryColumns><QueryColumn><Name>Date</Name><TableName></TableName><ColumnName>day_work_entries.workEntryDate as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>WorkHours</Name><TableName></TableName><ColumnName>day_work_entries.workHour as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Name</Name><TableName></TableName><ColumnName>users.name as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Project</Name><TableName></TableName><ColumnName>projects.name as</ColumnName><